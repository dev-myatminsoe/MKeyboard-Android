<?xml version="1.0" encoding="utf-8"?>
<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="@dimen/key_normal"
    android:keyWidth="10%p"
    android:verticalGap="3dp">
    <Row android:rowEdgeFlags="top">
        <Key
            android:codes="91"
            android:keyEdgeFlags="left"
            android:keyLabel="[" />
        <Key
            android:codes="93"
            android:keyLabel="]" />
        <Key
            android:codes="123"
            android:keyLabel="{" />
        <Key
            android:codes="125"
            android:keyLabel="}" />
        <Key
            android:codes="35"
            android:keyLabel="#" />
        <Key
            android:codes="37"
            android:keyLabel="%" />
        <Key
            android:codes="94"
            android:keyLabel="^" />
        <Key
            android:codes="42"
            android:keyLabel="*" />
        <Key
            android:codes="43"
            android:keyLabel="+" />
        <Key
            android:codes="61"
            android:keyEdgeFlags="right"
            android:keyLabel="=" />
    </Row>
    <Row>
        <Key
            android:codes="95"
            android:keyEdgeFlags="left"
            android:keyLabel="_" />
        <Key
            android:codes="92"
            android:keyLabel="\\" />
        <Key
            android:codes="124"
            android:keyLabel="|" />
        <Key
            android:codes="126"
            android:keyLabel="~" />
        <Key
            android:codes="60"
            android:keyLabel="&lt;" />
        <Key
            android:codes="62"
            android:keyLabel="&gt;" />

        <Key
            android:codes="162"
            android:keyLabel="¢" />
        <Key
            android:codes="163"
            android:keyLabel="£" />
        <Key
            android:codes="165"
            android:keyLabel="¥" />

        <Key
            android:codes="183"
            android:keyEdgeFlags="right"
            android:keyLabel="·" />
    </Row>
    <Row>
        <Key
            android:codes="-1"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyLabel="123"
            android:keyWidth="13%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="14.8%p" />
        <Key
            android:codes="44"
            android:keyLabel=","
            android:keyWidth="14.8%p" />
        <Key
            android:codes="63"
            android:keyLabel="\?"
            android:keyWidth="14.8%p" />
        <Key
            android:codes="33"
            android:keyLabel="!"
            android:keyWidth="14.8%p" />
        <Key
            android:codes="39"
            android:keyLabel="'"
            android:keyWidth="14.8%p" />

        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="13%p" />

    </Row>
    <Row android:rowEdgeFlags="bottom">

        <Key
            android:codes="-2"
            android:keyEdgeFlags="left"
            android:keyHeight="@dimen/key_bottom"
            android:keyLabel="abc"
            android:keyWidth="13%p" />

        <Key
            android:codes="-100"
            android:keyHeight="@dimen/key_bottom"
            android:keyIcon="@drawable/sym_keyboard_language_switch"
            android:keyWidth="12%p" />
        <Key
            android:codes="32"
            android:keyHeight="@dimen/key_bottom"
            android:keyLabel="space"
            android:keyWidth="60%p" />

        <Key
            android:codes="-4"
            android:keyEdgeFlags="right"
            android:keyHeight="@dimen/key_bottom"
            android:keyLabel="Go"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>